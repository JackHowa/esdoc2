{
  "name": "esdoc2",
  "version": "2.0.0",
  "description": "Better Documentation Generator For JavaScript",
  "author": {
    "name": "Matt McFarland",
    "url": "https://github.com/esdoc2/esdoc2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/esdoc2/esdoc2"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "test": "node ./scripts/test.js",
    "test-ci": "node ./scripts/test-ci.js",
    "esdoc": "node ./scripts/esdoc.js",
    "lint": "node ./scripts/eslint.js --fix",
    "start": "DEBUG=* ; static out/docs || export",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "babel-generator": "6.11.4",
    "babel-traverse": "6.12.0",
    "babylon": "6.14.1",
    "big-json": "^1.2.0",
    "cheerio": "0.22.0",
    "color-logger": "0.0.3",
    "escape-html": "1.0.3",
    "fs-extra": "1.0.0",
    "ice-cap": "0.0.4",
    "marked": "0.3.6",
    "minimist": "1.2.0",
    "taffydb": "2.7.2"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-plugin-istanbul": "2.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.11.5",
    "babel-register": "6.11.6",
    "codecov": "1.0.1",
    "esdoc2-accessor-plugin": "2.0.0",
    "esdoc2-external-ecmascript-plugin": "2.0.0",
    "esdoc2-importpath-plugin": "2.0.0",
    "esdoc2-standard-plugin": "2.0.0",
    "esdoc2-undocumented-identifier-plugin": "2.0.0",
    "eslint": "3.6.0",
    "http-server": "0.9.0",
    "mocha": "2.5.3",
    "node-static": "^0.7.10",
    "nyc": "8.3.0"
  },
  "keywords": [
    "jsdoc",
    "esdoc2",
    "api",
    "document",
    "documentation",
    "ES6"
  ],
  "files": [
    "out/src",
    "README.md"
  ],
  "main": "out/src/esdoc2.js",
  "bin": {
    "esdoc2": "out/src/ESDocCLI.js"
  }
}
